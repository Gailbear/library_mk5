<table class="table table-striped">
    <tr>
        <th class="col-xs-3"></th>
        <th class="col-xs-3">
            Title
        </th>
        <th class="col-xs-3">
            Checkouts
        </th>
        <th class="col-xs-3"></th>
    </tr>
    <% @event.checkouts_by_title.each_with_index do |s_row, index| %>
        <tr data-event-id="<%= index %>">
            <td class="col-xs-3"></td>
            <td class="col-xs-3">
                <%= s_row['title'] %>
            </td>
            <td class="col-xs-3">
                <%= s_row['checkouts'] %>
            </td>
            <td class="col-xs-3"></td>
        </tr>
    <% end %>
</table>